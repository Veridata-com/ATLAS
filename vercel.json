{
    "buildCommand": "npx expo export -p web",
    "outputDirectory": "dist",
    "devCommand": "npx expo start --web",
    "installCommand": "npm install && chmod -R 755 node_modules/.bin",
    "framework": null,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}